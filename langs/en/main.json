{
  "thisisa": "grammar",
  "language": "en",
  "syntax": {
    "adv": {
      "thisisa": "syntaxrule",
      "next": ["adj-noun"],
      "nodes": "[$desc{[verbal true]} $(desc verb)]",
      "function": "+[#1 ${[descs [#0]]}]"
    },
    "adj-noun": {
      "thisisa": "syntaxrule",
      "next": [],
      "nodes": "[$desc {[verbal false]} $noun]",
      "function": "+[#1 ${[descs [#0]]}]"
    },
    "art-noun": {
      "thisisa": "syntaxrule",
      "next": ["prep-noun"],
      "mandatory": true,
      "nodes": "[$article {[definite @1]} $noun {!definite}]",
      "function": "+[#1 ${[definite @1]}]"
    },
    "aux-verb": {
      "thisisa": "syntaxrule",
      "next": ["subject", "object", "object2"],
      "nodes": "[$aux-verb {[pat @0] [give @1]} @0]",
      "function": "+[#1 @1]"
    },
    "subject": {
      "thisisa": "syntaxrule",
      "next": ["object"],
      "nodes": [
        "$(noun desc) {[verbal (false null)]}",
        "$verb {[finite true] [personal true] !subject}"
      ],
      "function": "+[#1 ${[subject #0]}]"
    }
  },
  "morphology": {
    "article": {
      "thisisa": "litdict",
      "words": {
        "a": false,
        "an": false,
        "the": true
      },
      "function": "$article{[lang en] [definite #0]}"
    },
    "aux-verb": {
      "thisisa": "load",
      "list": "aux-verbs.json",
      "function": "+[$aux-verb{[lang en]} #0]",
    },
    "noun": {
      "thisisa": "load",
      "list": "nouns.json",
      "function": "$noun{[lang en] [is #0] [countable true] [proper false] [plural false]}"
    },
    "plural-noun": {
      "thisisa": "morphologyrule",
      "pat": "^(.*)s$",
      "replace": "$1",
      "next": ["noun", "proper"],
      "function": "+[${[plural true]} #0]"
    },
    "punct": {
      "thisisa": "litdict",
      "words": {
        ",": "comma",
        ".": "period",
        "!": "exclamation-point",
        "?": "question-mark",
        "–": "M-dash"
      },
      "function": "$punct {[lang en] [is #0]}"
    },
    "quot": {
      "thisisa": "litdict",
      "words": {
        "(": "${[is paren] [open true]}",
        ")": "${[is paren] [open false]}",
        "'": [
          "${[is single] [open true]}",
          "${[is single] [open false]}"
        ],
        "\"": [
          "${[is double] [open true]}",
          "${[is double] [open false]}"
        ],
        "‘": "${[is single] [open true]}",
        "’": "${[is single] [open false]}",
        "“": "${[is double] [open true]}",
        "”": "${[is double] [open false]}"
      },
      "function": "+[#0 quot]"
    }
  }
}
