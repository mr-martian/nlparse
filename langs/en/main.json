{
  "thisisa": "grammar",
  "language": "en",
  "syntax": {
    "adv": "?[$adv $(adj adv verb)] *[#1 descs [#0]] [adj-noun]",
    "adj-noun": "?[$adj $noun] *[#1 descs [#0]] []",
    "art-noun": "?[$article{[definite @1]} $noun{!definite !possessor}] *[#1 definite @1] [prep-noun]",
    "aux-verb": "?[$aux-verb{[pat @0] [give @1]} @0] +[#1 @1] [subject object]",
    "conj-after": "?[$conjunction{!after} $@0] +[#0 ${[after #1] [matchas $@0]}] [conj-just-before conj-before]",
    "conj-before": "?[$@0 $punct{[is comma]} $conjunction{[after $@0]}] *[#2 before [#0]] [conj-before]",
    "conj-just-before": "?[$@0 $conjunction{[after $@0] !just-before !before}] *[#1 just-before [#0]] [conj-before]",
    "noun-prep": "?[$noun $preposition{clause}] *[#0 descs [#1]] [prep-noun]",
    "object": "?[$verb{!object [valency (transitive ditransitive)]} $(noun adj)] *[#0 object #1] [object2 verb-prep verb-com-prep]",
    "object2": "?[$verb{object [valency ditransitive]} $(noun adj)] *[#0 object2 #1] [verb-prep verb-com-prep]",
    "possessive": "?![$noun{[possessive true]} $noun{!possessor !definite}] +[#1 ${[possessor #0] [definite true]}] [prep-noun]",
    "prep-noun": "?[$preposition{!clause} $noun] *[#0 clause #1] [noun-prep]",
    "prep-com-verb": "?[$preposition{clause} $punct{[is comma]} $verb] *[#2 descs [#0]] []",
    "prep-verb": "?[$preposition{clause} $verb] *[#1 descs [#0]] [prep-com-verb]",
    "punct": "?[$verb $punct{[end true]}] *[#0 punct #1] []",
    "subject": "?[$(noun adj) $verb{[finite true] [personal true] !subject}] *[#1 subject #0] [object prep-verb prep-com-verb]",
    "verb-com-prep": "?[$verb{!punct} $punct{[is comma]} $preposition{clause}] *[#0 descs [#2]] [punct]",
    "verb-prep": "?[$verb{!punct} $preposition{clause}] *[#0 descs [#1]] [verb-com-prep punct]"
  },
  "morphology": {
    "article": {
      "thisisa": "list",
      "words": {
        "a": false,
        "an": false,
        "the": true
      },
      "function": "$article{[definite #0]}",
      "decapitalize": true
    },
    "aux-verb": {
      "thisisa": "list",
      "file": "aux-verbs.json",
      "function": "+[$aux-verb #0]",
      "decapitalize": true
    },
    "conjunction": {
      "thisisa": "list",
      "words": [
        "and",
        "but",
        "nor",
        "or",
        "so",
        "yet"
      ],
      "function": "$conjunction{[is #0]}",
      "decapitalize": true
    },
    "noun": {
      "thisisa": "list",
      "file": "nouns.json",
      "function": "$noun{[is #0] [countable true] [proper false] [plural false]}",
      "decapitalize": true
    },
    "past": {
      "thisisa": "morphologyrule",
      "pat": "^([a-z]+)ed(( [a-z]+)*)",
      "replace": "$1$2",
      "next": ["transitive"],
      "function": "*[#0 tense past]",
      "decapitalize": true
    },
    "past-keep-e": {
      "thisisa": "morphologyrule",
      "pat": "^([a-z]+e)d(( [a-z]+)*)",
      "replace": "$1$2",
      "next": ["transitive"],
      "function": "*[#0 tense past]",
      "decapitalize": true
    },
    "plural-noun": {
      "thisisa": "morphologyrule",
      "pat": "^(.*)s$",
      "replace": "$1",
      "next": ["noun", "proper"],
      "function": "*[#0 plural true]"
    },
    "prep": {
      "thisisa": "list",
      "file": "prepositions.json",
      "function": "$preposition{[is #0]}",
      "decapitalize": true
    },
    "punct": {
      "thisisa": "list",
      "words": {
        ",": "${[is comma] [end false]}",
        ".": "${[is period] [end true]}",
        "!": "${[is exclamation-point] [end true]}",
        "?": "${[is question-mark] [end true]}",
        "–": "${[is M-dash] [end false]}"
      },
      "function": "+[#0 $punct]"
    },
    "quot": {
      "thisisa": "list",
      "words": {
        "(": "${[is paren] [open true]}",
        ")": "${[is paren] [open false]}",
        "'": [
          "${[is single] [open true]}",
          "${[is single] [open false]}"
        ],
        "\"": [
          "${[is double] [open true]}",
          "${[is double] [open false]}"
        ],
        "‘": "${[is single] [open true]}",
        "’": "${[is single] [open false]}",
        "“": "${[is double] [open true]}",
        "”": "${[is double] [open false]}"
      },
      "function": "+[#0 $quot]"
    },
    "transitive": {
      "thisisa": "list",
      "file": "transitives.json",
      "function": "$verb{[is #0] [tense present] [finite true] [valency transitive] [mood indicative] [personal true]}",
      "decapitalize": true
    }
  },
  "wordify": {
    "thisisa": "wordify",
    "words": [
      "[A-Z][a-z]*",
      "[a-z]+",
      "[A-Z]?[a-z]+n['’]t",
      "['’](em|d|ve|s|re|ll)",
      "(I['’]m|[lL]et['’]s|[oO]['’](clock)?|['’][tT]was)",
      "[,.'\"‘’“”()!?\\-:]",
      "[0-9]+(\\.[0-9]*)?(th)?",
      "[0-9]{1,3}(,[0-9]{3})+(th)?",
      "[A-Z]?[a-z]+ (aback|about|across|after|(up |out |)against|ahead|along( with)?|apart|around|aside|astray|(back )?at|away( for| from| with|)|back|before|behind|between|by|down( for)?|for|forth|forward( to)?|from|in( for| with| on|)|into|low|(|out |hold |rid |means |back |case |front |place |spite |top )of|off|on|onto|out(| of| from( under)?)|over|rid|round|through( with)?|(addition |)to|together|towards|under|up( to|)|upon|way|with|without)"
    ],
    "skip": "\\s"
  },
  "reject": [
    "$adv",
    "$article",
    "$conjunction{!after}",
    "$verb{[valency (transitive ditransitive)] !object}",
    "$verb{[valency ditransitive] !object2}",
    "$verb{[personal true] !subject}",
    "$punct",
    "$preposition{!clause}"
  ]
}
