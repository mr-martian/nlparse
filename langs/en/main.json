{
  "thisisa": "grammar",
  "language": "en",
  "syntax": {
    "adv": {
      "thisisa": "syntaxrule",
      "next": ["adj-noun"],
      "nodes": [
        {
          "thisisa": "node",
          "type": "desc",
          "verbal": true
        },
        {
          "thisisa": "node",
          "type": {
            "thisisa": "or",
            "options": ["desc", "verb"]
          }
        }
      ],
      "function": {
        "thisisa": "function",
        "function": "add",
        "node": {
          "thisisa": "noderef",
          "node": 1
        },
        "key": "descs",
        "val": {
          "thisisa": "noderef",
          "node": 0
        }
      }
    },
    "adj-noun": {
      "thisisa": "syntaxrule",
      "next": [],
      "nodes": [
        {
          "thisisa": "node",
          "type": "desc",
          "verbal": false
        },
        {
          "thisisa": "node",
          "type": "noun"
        }
      ],
      "function": {
        "thisisa": "function",
        "function": "add",
        "node": {
          "thisisa": "noderef",
          "node": 1
        },
        "key": "descs",
        "val": {
          "thisisa": "noderef",
          "node": 0
        }
      }
    },
    "art-noun": {
      "thisisa": "syntaxrule",
      "next": ["prep-noun"],
      "mandatory": true,
      "nodes": [
        {
          "thisisa": "node",
          "type": "article",
          "definite": {
            "thisisa": "wildcard",
            "id": 1
          }
        },
        {
          "thisisa": "node",
          "type": "noun",
          "definite": null
        }
      ],
      "--function": {
        "thisisa": "function",
        "function": "set",
        "node": {
          "thisisa": "noderef",
          "node": 1
        },
        "key": "definite",
        "val": {
          "thisisa": "wildcard",
          "id": 1
        }
      },
      "function": {
        "thisisa": "merge",
        "things": [
          {
            "thisisa": "noderef",
            "node": 1
          },
          {
            "definite": {
              "thisisa": "wildcard",
              "id": 1
            }
          }
        ]
      }
    },
    "aux-verb": {
      "thisisa": "syntaxrule",
      "next": ["subject", "object", "object2"],
      "nodes": [
        {
          "thisisa": "node",
          "type": "aux-verb",
          "pat": {
            "thisisa": "wildcard",
            "id": 0
          },
          "give": {
            "thisisa": "wildcard",
            "id": 1
          }
        },
        {
          "thisisa": "wildcard",
          "id": 0
        }
      ],
      "function": {
        "thisisa": "function",
        "function": "merge",
        "from": {
          "thisisa": "wildcard",
          "id": 1
        },
        "to": {
          "thisisa": "noderef",
          "node": 1
        }
      }
    },
    "subject": {
      "thisisa": "syntaxrule",
      "next": ["object"],
      "nodes": [
        {
          "thisisa": "node",
          "type": {
            "thisisa": "or",
            "options": ["noun", "desc"]
          },
          "verbal": {
            "thisisa": "or",
            "options": [false, null]
          }
        },
        {
          "thisisa": "node",
          "type": "verb",
          "finite": true,
          "personal": true,
          "subject": null
        }
      ],
      "function": {
        "thisisa": "function",
        "function": "set",
        "node": {
          "thisisa": "noderef",
          "node": 1
        },
        "key": "subject",
        "val": {
          "thisisa": "noderef",
          "node": 0
        }
      }
    }
  },
  "morphology": {
    "article": {
      "thisisa": "litdict",
      "words": {
        "a": false,
        "an": false,
        "the": true
      },
      "function": {
        "thisisa": "node",
        "type": "article",
        "lang": "en",
        "definite": {
          "thisisa": "noderef",
          "node": 0
        }
      }
    },
    "aux-verb": {
      "thisisa": "load",
      "list": "aux-verbs.json",
      "function": {
        "thisisa": "function",
        "function": "merge",
        "from": {
          "thisisa": "noderef",
          "node": 0
        },
        "to": {
          "thisisa": "node",
          "type": "aux-verb",
          "lang": "en"
        }
      }
    },
    "noun": {
      "thisisa": "load",
      "list": "nouns.json",
      "function": {
        "thisisa": "node",
        "lang": "en",
        "type": "noun",
        "is": {
          "thisisa": "noderef",
          "node": 0
        },
        "countable": true,
        "proper": false,
        "plural": false
      }
    },
    "plural-noun": {
      "thisisa": "morphologyrule",
      "pat": "^(.*)s$",
      "replace": "$1",
      "next": ["noun", "proper"],
      "function": {
        "thisisa": "function",
        "function": "set",
        "node": {
          "thisisa": "noderef",
          "node": 0
        },
        "key": "plural",
        "val": true
      }
    },
    "punct": {
      "thisisa": "litdict",
      "words": {
        ",": "comma",
        ".": "period",
        "!": "exclamation-point",
        "?": "question-mark",
        "–": "M-dash"
      },
      "function": {
        "thisisa": "node",
        "type": "punct",
        "lang": "en",
        "is": {
          "thisisa": "noderef",
          "node": 0
        }
      }
    },
    "quot": {
      "thisisa": "litdict",
      "words": {
        "(": {
          "is": "paren",
          "open": true
        },
        ")": {
          "is": "paren",
          "open": false
        },
        "'": [
          {
            "is": "single",
            "open": true
          },
          {
            "is": "single",
            "open": false
          }
        ],
        "\"": [
          {
            "is": "double",
            "open": true
          },
          {
            "is": "double",
            "open": false
          }
        ],
        "‘": {
          "is": "single",
          "open": true
        },
        "’": {
          "is": "single",
          "open": false
        },
        "“": {
          "is": "double",
          "open": true
        },
        "”": {
          "is": "double",
          "open": false
        }
      },
      "function": {
        "thisisa": "function",
        "function": "merge",
        "from": {
          "thisisa": "noderef",
          "node": 0
        },
        "to": {
          "thisisa": "node",
          "type": "quot"
        }
      }
    }
  }
}
